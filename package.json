{
  "name": "@stordco/msw-toolbar",
  "author": "STORD",
  "version": "0.1.4",
  "license": "MIT",
  "homepage": "https://github.com/stordco/msw-toolbar",
  "repository": "https://github.com/stordco/msw-toolbar.git",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "msw": "^0.35.0",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/msw-toolbar.esm.js",
  "size-limit": [
    {
      "path": "dist/msw-toolbar.cjs.production.min.js",
      "limit": "4 KB"
    },
    {
      "path": "dist/msw-toolbar.esm.js",
      "limit": "4 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.0",
    "msw": "^0.35.0",
    "postcss": "^8.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "size-limit": "^5.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
